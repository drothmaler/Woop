  <PropertyGroup>
    <PreBuildEvent>del /q "$(SolutionDir)Woop\Assets\Scripts\"
xcopy /y /e "$(SolutionDir)submodules\Boop\Boop\Boop\scripts" "$(SolutionDir)Woop\Assets\Scripts\"</PreBuildEvent>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="App.xaml.old.cs" />
